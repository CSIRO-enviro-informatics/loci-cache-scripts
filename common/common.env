# Setting a heap of environment variables that will be common across these docker containers
# These can be `source`ed into scripts

S3_BUCKET="loci-assets"
S3_REGION="ap-southeast-2"

#This is the used to create and consume the datasets 
S3_DATASET_PATH="/auto-generated/datasets"
ASGS2016_DATASET_FILE="asgs2016.trig.gz"
ASGS2011_DATASET_FILE="asgs2011.trig.gz"
GEOFABRIC_DATASET_FILE="geofabric.trig.gz"
GNAF_DATASET_FILE="gnaf_current.trig.gz"
GNAF1605_DATASET_FILE="gnaf_1605.trig.gz"

#This is the used to create and consume the linksets 
S3_LINKSET_PATH="/linksets"
MB2CC_LINKSET_FILE="ls_mb16cc.ttl"
ADDR2CC_LINKSET_FILE="ls_addrcatch.trig"
MB11MB16_LINKSET_FILE="ls_mb11mb16.trig.gz"
